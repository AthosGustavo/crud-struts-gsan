<!DOCTYPE struts-config PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
    "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="clienteForm" type="crud.actionform.ClienteForm" />
	</form-beans>

	<action-mappings>
		<action path="/cadastro/cliente" name="clienteForm" type="crud.actionclass.ClienteAction" input="/formulario.jsp">
			
		</action>
	</action-mappings>

	<message-resources parameter="crud.mensagens.MsgCustomizadas"></message-resources>

</struts-config>
	